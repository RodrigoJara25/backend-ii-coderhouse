<h1>{{title}}</h1>

<form action="/api/sessions/register" method="POST">
    <div>
        <label for="first_name">Nombre: </label>
        <br>
        <input type="text" id="first_name" name="first_name" placeholder="Rodrigo">
    </div>
    <div>
        <label for="last_name">Apellido: </label>
        <br>
        <input type="text" id="last_name" name="last_name" placeholder="Jara">
    </div>
    <div>
        <label for="email">Email: </label>
        <br>
        <input type="email" id="email" name="email" placeholder="nombre@gmail.com">
    </div>
    <div>
        <label for="age">Edad: </label>
        <br>
        <input type="number" id="age" name="age" placeholder="21">
    </div>
    <div>
        <label for="password">Password: </label>
        <br>
        <input type="password" id="password" name="password" placeholder="********">
    </div>
    <br>
    <button type="submit">Registrar</button>
</form>
